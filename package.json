{
  "name": "react-flexview",
  "version": "0.0.1",
  "description": "A react component to abstract over flexbox",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "build": "rm -rf lib && mkdir lib && node-sass src --importer sass-importer.js --include-path node_modules -o lib && babel src -d lib",
    "lint": "eslint src dev/examples.js dev/examples",
    "preversion": "npm run lint && npm run test && npm run build-examples",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run build",
    "build-examples": "npm run clean && webpack --config dev/webpack.config.build.babel.js --progress",
    "start": "npm run clean && webpack-dev-server --config dev/webpack.config.babel.js --progress --hot --inline",
    "clean": "rm -rf dev/build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:buildo/react-flexview"
  },
  "keywords": [
    "react",
    "react-component",
    "flex",
    "flexbox"
  ],
  "author": "Francesco Cioria <francesco@buildo.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/buildo/react-flexview/issues"
  },
  "homepage": "https://github.com/buildo/react-flexview",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.13.1",
    "sass-flex-mixin": "^1.0.3",
    "tcomb": "^3.1.0",
    "tcomb-react": "^0.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "buildo-react-components": "github:buildo/react-components",
    "component-playground": "^1.2.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-buildo": "github:buildo/eslint-config",
    "eslint-loader": "^1.3.0",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^1.7.0",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-remarkable": "^1.1.1",
    "react-router": "^0.13.5",
    "react-sidebar": "^2.1.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "sass-variables-loader": "0.1.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
